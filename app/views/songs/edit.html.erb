<h2>Edit Song</h2>
<%=form_for @song do |f|%>
  <%if @song.errors.any?%>
    <h2>
      <%= pluralize(@song.errors.count, "error")%>
      prohibited this song from being saved:
    </h2>
    <ul>
    <% @song.errors.full_messages.each do |message|%>
      <li><%=message%></li>
    <%end%>
    </ul>
  <%end%>

  <%= f.text_field :title%>
  <%= f.text_field :artist_name%>
  <%=f.check_box :released%>
  <%=f.text_field :release_year%>
  <%=f.text_field :genre%>
  <%= f.submit%>
<%end%>
