<h3>Edit a Song</h3>

<%= form_for(@song) do |f| %>
  <%= f.label "Title" %><br>
  <%= f.text_field :title %><br>

  <%= f.label "Released?" %><br>
  <%= f.check_box :released %><br>

  <%= f.label "Release year" %><br>
  <%= f.text_field :release_year %><br>

  <%= f.label "Artist's name" %><br>
  <%= f.text_field :artist_name %><br>

  <%= f.label "Genre" %><br>
  <%= f.text_field :genre %><br>

  <%= f.submit %>
  <p></p>
  <%= link_to 'Back', songs_path %>
<% end %>




<%= params.inspect %>
