<h2>Edit a Song</h2>
<% if @song.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@song.errors.count, "error") %> prohibited this post from being saved:</h2>
    <ul>
    <% @song.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= form_for @song do |f| %>
  Title: <%= f.text_field :title %><br />
  Genre: <%= f.text_field :genre %><br />
  Artist Name: <%= f.text_field :artist_name %><br />
  Release Year: <%= f.number_field :release_year %><br />
  Released? <%= f.check_box :released %><br />
<%= f.submit %>
<% end %>


