<h1>Edit Song</h1>
<% if @song.errors.any? %>
<div id="error_explanation">
  <h2> There were some errors: </h2>
  <ul>
    <% @song.error.full_messages.each do |error_message| %>
    <li><%= error_message %></li>
    <% end %>
  </ul>
</div>
<% end %>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name%>
  <%= f.text_field :artist_name %>
  <%= f.label :release_year%>
  <%= f.text_field :release_year %>
  <%= f.label :released%>
  <%= f.check_box :released %>
  <%= f.label :genre%>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
