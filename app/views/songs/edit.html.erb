<h1>Edit Song</h1>
<%= form_for @song do |f| %>
<h3 style="display: inline">Song Name:</h3><br>
<%= f.text_field :title %><br>
<h3 style="display: inline">Artist Name:</h3><br>
<%= f.text_field :artist_name %><br>
<h3 style="display: inline">Released:</h3><br>
<%= f.check_box :released %><br>
<h3 style="display: inline">Release Year:</h3><br>
<%= f.text_field :release_year %><br>
<h3 style="display: inline">Genre:</h3><br>
<%= f.text_field :genre %><br>
<%= f.submit %>
<% end %>
