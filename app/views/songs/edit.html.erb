<h2>Editing "<%= @song.title %>"</h2>

<%= form_for @song do |f| %>
<% if @song.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@song.errors.count, "error") %>
      prohibited this post from being saved:
    </h2>
 
    <ul>
    <% @song.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
  <%=f.label :title %>
  <%= f.text_field :title %>
  <%=f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%=f.label :genre %>
  <%= f.text_field :genre %>
  <%=f.label :released %>
  <%= f.check_box :released %>
  <%=f.label :release_year %>
  <%= f.text_field :release_year %>
  <%= f.submit %>
<% end %>
<%params.inspect%>