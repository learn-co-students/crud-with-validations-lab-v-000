Edit Song<br>
<%= form_for @song do |f| %>
<%= f.label :title %>
<%= f.text_field :title, f.title %>

<%= f.label :released %>
<%= f.text_field :released, f.released %>

<%= f.label :release_year %>
<%= f.text_field :release_year, f.release_year %>

<%= f.label :artist_name %>
<%= f.text_field :artist_name, f.artist_name %>

<%= f.label :genre %>
<%= f.text_field :genre, f.genre %>

<%= f.submit "Create Song"%>
<%end%>
