<%= form_for @song do |f| %>
  <p>
  <%= f.label :title %>
  <%= f.text_field :title%>
  </p>

  <p>
    <%= f.label :released %>
    <%= f.check_box :released%>
  </p>
  

  <%= f.label :release_year %>
  <%= f.text_field :release_year%>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name%>

  <%= f.label :genre %>
  <%= f.text_field :genre%>

  <%= f.submit %>

<% end %>