Edit Song

<%= form_for(@song) do |f| %>
  <%= f.text_field :title %>
  <%= f.check_box :released %>
  <%= f.text_field :release_year %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :genre %>
  <%= f.submit "Update Song" %>
<% end %>
<%= link_to"Delete", @song, method: :delete, data: {confirm: "Really?"} %>