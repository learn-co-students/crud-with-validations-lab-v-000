<h2>Editing "<%= @song.title %>"</h2>
<%if @song.errors.any?%>

  <div id="error_explanation">
    <ul>
      <%@song.errors.full_messages.each do |message|%>
      <li id="error_explanation"><%=message%></li>
      
      <%end%>
    </ul>
  </div>
<%end%>

<%=form_for(@song) do |f|%>
	<%=f.label :title%>
	<%=f.text_field :title%>
	<br>
	<%=f.label :artist_name%>
	<%=f.text_field :artist_name%>
	<br>
	<%=f.label :genre%>
	<%=f.text_field :genre%>
	<br>
	<%=f.label :released%>
	<%=f.check_box :released%>
	<br>
	<%=f.label :release_year%>
	<%=f.text_field :release_year%>
	<%=f.submit "Update Song"%>
<%end%>