<%= form_for @song do |f| %>
<label>Title:</label>
<%= f.text_field :title %><br>
<label>Released:</label>
<%= f.check_box :released %><br>
<label>Release Year:</label>
<%= f.text_field :release_year %><br>
<label>Artist:</label>
<%= f.text_field :artist_name %><br>
<label>Genre:</label>
<%= f.text_field :genre %><br>
<%= f.submit "Create Song" %>
<% end %>
