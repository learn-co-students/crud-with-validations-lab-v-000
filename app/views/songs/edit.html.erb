<h1>Edit <%=@song.title%></h1>
<%= link_to "Delete", @song, method: :delete, data: { confirm: "Really?" }%>
<%= form_for @song do |f| %>
	<%= f.text_field :title%>
	<%= f.text_field :artist_name%>
	<%= f.check_box :released%>
	<%= f.text_field :release_year%>
	<%= f.text_field :genre%>
	<%= f.submit %>
<%end%>
