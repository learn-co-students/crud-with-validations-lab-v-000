<h1>Song Form</h1>
<%= form_for @song do |f| %>
  <%= f.label 'Title' %> <%= f.text_field :title %>
  <% f.label 'Released' %> <%= f.check_box :released %>
  <%= f.label 'Release Year' %> <%= f.text_field :release_year %>
  <%= f.label 'Artist Name' %> <%= f.text_field :artist_name %>
  <%= f.label 'Genre' %> <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>