<h1>Edit a Song</h1>

<%= form_for @song do |f| %>

  <p><label>Title</label><br>
  <%= f.text_field :title %></p>

  <p><label>Released?</label><br>
  <%= f.check_box :released %></p>

  <p><label>Release Year</label><br>
  <%= f.text_field :release_year %></p>

  <p><label>Artist Name</label><br>
  <%= f.text_field :artist_name %></p>

  <p><label>Genre</label><br>
  <%= f.text_field :genre %></p>

  <%= f.submit %>

<% end %>