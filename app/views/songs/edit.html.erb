Edit <%= @song.title %> <br>

<%= form_for(@song) do |f| %>
    <%= f.label "Title " %>
    <%= f.text_field :title %><br>
    <%= f.label "Release Year "  %>
    <%= f.text_field :release_year %><br>
    <%= f.label "Artist Name "  %>
    <%= f.text_field :artist_name %><br>
    <%= f.label "Genre "  %>
    <%= f.text_field :genre %><br>
    <%= f.submit %>
<% end %>
