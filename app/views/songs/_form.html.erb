<%= form_for @song do |s| %>
  <%= s.label :title %>
  <%= s.text_field :title %>
  <%= s.label :release_year %>
  <%= s.text_field :release_year %>
  <%= s.label :released  %>
  <%= s.check_box :released %>
  <%= s.label :genre %>
  <%= s.text_field :genre %>
  <%= s.label :artist_name %>
  <%= s.text_field :artist_name %>
  <%= s.submit  %>
<% end %>
