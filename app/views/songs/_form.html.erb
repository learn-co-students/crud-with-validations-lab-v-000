<%= form_for @song do |f| %>
  Song Title: <%= f.text_field :title %><br>
  Check box if song is released:<%= f.check_box :released %><br>
  Release Year:<%= f.text_field :release_year %><br>
  Artist Name:<%= f.text_field :artist_name %><br>
  Genre:<%= f.text_field :genre %><br>
  <%= f.submit "Submit form" %>
<% end %>
