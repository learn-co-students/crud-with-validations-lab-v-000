	<% if @song.errors.any? %>	    <div id="error_explanation">	        <h2>	        <%= pluralize(@song.errors.count, "error") %>	        prohibited this post from being saved:	        </h2>	 	        <ul>	        <% @song.errors.full_messages.each do |msg| %>	            <li><%= msg %></li>	        <% end %>	        </ul>	    </div>	<% end %>		<%= form_for @song do |f| %>	<label>Song Title:</label><br><br>	    <%= f.text_field :title %><br><br>	 <label>Check if already released:</label>	    <%= f.check_box :released %><br><br>	 <label>Release Year:</label><br><br>	    <%= f.text_field :release_year %><br><br>	 <label>Artist Name:</label><br><br>	    <%= f.text_field :artist_name %><br><br>	  <label>Genre:</label><br><br>	    <%= f.text_field :genre %><br><br>	    <%= f.submit %>	<% end %>