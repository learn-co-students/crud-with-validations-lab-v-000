<%=form_for(@song) do |f|%>
  <%=f.label :title, "Title:"%>
  <%=f.text_field :title%><br></br>
  <%=f.label :artist_name, "Artist Name:"%>
  <%=f.text_field :artist_name%><br></br>
  <%=f.label :released, "Released: "%>
  <%=f.check_box :released, checked: @song.released%><br></br>
  <%=f.label :release_year, "Release Year"%>
  <%=f.text_field :release_year%><br></br>
  <%=f.label :genre, "Genre"%>
  <%=f.text_field :genre%><br></br>
  <%=f.submit%>
<%end%>
