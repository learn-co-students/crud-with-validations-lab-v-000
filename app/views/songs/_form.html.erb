<%= form_for(@song) do |f| %>
    <%= f.label :title, "Title" %>
    <%= f.text_field :title %>

    <%= f.label :artist_name, "Artist Name" %>
    <%= f.text_field :artist_name %>

    <%= f.label :release_year, "Release Year" %>
    <%= f.number_field :release_year %>

    <%= f.label :released, "Released" %>
    <%= f.check_box :released %>

    <%= f.label :genre, "Genre" %>
    <%= f.text_field :genre %>

    <%= f.submit %>

<% end %>