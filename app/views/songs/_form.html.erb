<%= form_for(@song) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :song_released %>
  <%= f.check_box :released %>
  <br>
  <%= f.label :song_release_year %>
  <%= f.text_field :release_year %>
  <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <br>
  <%= f.submit %>
<% end %>
