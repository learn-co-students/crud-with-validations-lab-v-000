<h1>Create Song</h1><br>
<%= form_for (@song) do |f| %>
<label>title</label><br>
<%= f.text_field :title %><br>
<label>released</label><br>
<%= f.text_field :released %><br>
<label>release_year</label><br>
<%= f.text_field :release_year %><br>
<label>artist_name</label><br>
<%= f.text_field :artist_name %><br>
<label>genre</label><br>
<%= f.text_field :genre %><br>

<%= f.submit 'Submit Song' %>
<%end%>
