<%= form_for(@song) do |s| %>
	<%= s.label :title %>
	<%= s.text_field :title %><br>

	<%= s.label :artist_name %>
	<%= s.text_field :artist_name %><br>

	<%= s.label :release_year %>
	<%= s.text_field :release_year %><br>

	<%= s.label :released %><br>
	<%= s.check_box :released, {}, true, false %>

	<%= s.label :genre %>
	<%= s.text_field :genre %><br>
	<%= s.submit %>
<% end %>