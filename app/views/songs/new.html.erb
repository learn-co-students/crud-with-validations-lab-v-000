<h2> Create a Song </h2>
<% if @song.errors.any? %>
<div id="error_explanation">
  <h2> There were some errors: </h2>
  <ul>
    <% @song.error.full_messages.each do |error_message|%>
    <li><%=error_message %></li>
    <% end %>
  </ul>
</div>
<% end %>


<%= form_for @song do |f| %>

<label> Title </label> <%= f.text_field :title %>
<label> Artist Name </label> <%= f.text_field :artist_name %>
<label> Release Year </label> <%= f.text_field :release_year %>
<label> Released </label> <%= f.check_box :released %>
<label> Genre </genre> <%= f.text_field :genre %>
<%= f.submit "Create" %>
<% end %>
