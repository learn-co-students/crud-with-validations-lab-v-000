<h1> Create a New Song</h1>

<%= form_for @song do |f| %>
  <p><%= f.label :title %>
  <%= f.text_field :title %><br></p>
  <p><%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br></p>
  <p><%= f.label :release_year %>
  <%= f.text_field :release_year %><br></p>
  <p><%= f.label :genre %>
  <%= f.text_field :genre %><br></p>
  <p><%= f.label :released %>
  <%= f.text_field :released %><br></p>
  <p><%= f.submit "Create" %></p>
<% end %>
