<h3>New Song</h3>

 <%= form_for(@song) do |f| %>
  <%= f.label 'Title' %>
  <%= f.text_field :title %>
  <%= f.label 'Release Year' %>
  <%= f.text_field :release_year %>
  <%= f.label 'Released' %>
  <%= f.text_field :released %>
  <%= f.label 'Genre' %>
  <%= f.text_field :genre %>
  <%= f.label 'Artsit Name' %>
  <%= f.text_field :artist_name %>

   <%= f.submit %>
<% end %>