<h3>New Song</h3>
<%= form_for @song do |f| %>
  Song Title: <%= f.text_field :title  %><br>
  Artist Name: <%= f.text_field :artist_name %><br>
  Genre: <%= f.text_field :genre %><br>
  Released: Check for Yes <%= f.check_box :released %><br>
  Year of Release <%= f.number_field :release_year %><br>
  <%= f.submit "Create Song" %>
<% end %>