
<h2>Song</h2>
<%= form_tag songs_path, method: "post" do %>
  <div class="field">
    <%= label_tag "title", "Title" %>
    <%= text_field_tag "title" %>
  </div>
  <div class="field">
    <%= label_tag "released", "Released" %>
    <%= text_field_tag "released" %>
  </div><div class="field">
    <%= label_tag "release_year", "Release_year" %>
    <%= text_field_tag "release_year" %>
  </div><div class="field">
    <%= label_tag "artist_name", "Artist_name" %>
    <%= text_field_tag "artist_name" %>
  </div>
  <div class="field">
    <%= label_tag "genre", "Genre" %>
    <%= text_field_tag "genre" %>
  </div>
  <%= submit_tag "Create Song" %>
<% end %>


