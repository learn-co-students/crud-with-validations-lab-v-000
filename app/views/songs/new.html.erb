<h1>New Song</h1>
<%= form_for @song do |f| %>
	<%= f.label :title, "Title" %>
	<%= f.text_field :title %><br>
	<%= f.label :released, "Released" %>
	<%= f.check_box :released %><br>
	<%= f.label :release_year, "Release year" %>
	<%= f.text_field :release_year %><br>
	<%= f.label :artist_name, "Artist name" %>
	<%= f.text_field :artist_name %><br>
	<%= f.label :genre, "Genre" %>
	<%= f.text_field :genre %><br>
	<%= submit_tag "Create Song" %>
<% end %>