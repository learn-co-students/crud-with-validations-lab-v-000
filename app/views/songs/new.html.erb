<h1> Create A Song</h1>

<% if @song.errors.any? %>
  <%= @song.errors.count %> <%= Errors %>
  <% @song.errors.full_messages.each do |error| %>
    <%= error %>
    <br>
    <% end %>
    <% end %>
<%= form_for @song do |f| %>
    <%= f.label :title%>
    <%= f.text_field :title%>
    <br>
    <%= f.label :released%>
    <%= f.text_field :released%>
    <br>
    <%= f.label :release_year%>
    <%= f.text_field :release_year%>
    <br>
    <%= f.label :artist_name%>
    <%= f.text_field :artist_name%>
    <br>
    <%= f.label :genre%>
    <%= f.text_field :genre%>
    <br>
    <%= f.submit %>
<% end %>
