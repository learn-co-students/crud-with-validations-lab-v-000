<%= form_for @song do |f| %>
    <p> Song Title: <%= f.text_field :title %> </p>
    <p> Released? <%= f.text_field :released %> </p>
    <p> Year Released: <%= f.text_field :release_year %> </p>
    <p> Artist Name: <%= f.text_field :artist_name %> </p>
    <p> Genre: <%= f.text_field :genre %> </p>
    <%= f.submit %>
<% end %>
