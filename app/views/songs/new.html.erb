<h1>Create New Song</h1>

 <% if @song.errors.any? %>
   <p><%= pluralize(@song.errors.count, "error") %>
     prohibited this song from being saved:</p>
   <ul>
     <% @song.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
     <% end %>
   </ul>
 <% end %>

 <%= form_for @song do |f| %>
   <%= f.label :title %>
   <%= f.text_field :title %><br>
   <%= f.label :artist_name %>
   <%= f.text_field :artist_name %><br>
   <%= f.label :released %>
   <%= f.check_box :released %><br>
   <%= f.label :release_year %>
   <%= f.text_field :release_year %><br>
   <%= f.label :genre %>
   <%= f.text_field :genre %><br>
   <%= f.submit %>
 <% end %>
