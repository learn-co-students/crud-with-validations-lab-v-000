<html>
<head>
  <title>songs new</title>
</head>
<body>
  <%= form_for(@song) do |f| %>
    <%= f.text_field :title %>
    <%= f.text_field :artist_name %>
    <%= f.text_field :genre %>
    <%= f.check_box :released %>
    <%= f.text_field :release_year %>
    <%= f.submit %>
  <% end %>

</body>
</html>
