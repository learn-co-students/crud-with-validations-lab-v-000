<%= form_for(@song) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
  </div>

  <div>
    <%= f.label :released %>
    <%= f.check_box :released %>
  </div>

  <div>
    <%= f.label :release_year %>
    <%= f.text_field :release_year %>
  </div>

  <div>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
