<h2>Create a Song</h2>
<%= form_for(@song) do |f| %>
<label>Song Title:</label>
  <%= f.text_field :title%></br>
<label>Song Release Year:</label>
  <%= f.number_field :release_year%></br>
<label>Song Artist Name:</label>
  <%= f.text_field :artist_name%></br>
<label>Song Genre:</label>
  <%= f.text_field :genre %></br>
<label>Song Released:</label>
  <%= f.text_field :released %></br>

  <%= f.submit %>
<% end %>
