<h2>Create a Song</h2>
<%= form_for @song do |f| %>
<% if @song.errors.any? %>
  <div id= "error_explanation">
  <h2>
      <%= pluralize(@song.errors.count, "error") %>
      prohibited this song from being saved:
    </h2>
    <h2>There were some errors:</h2>
    <ul>
        <% @song.errors.full_messages.each do |message| %>
        <li><%= message %> </li>
        <% end %>
    </ul>
  </div>
  <% end %>

    Title: <%= f.text_field :title %>
    Release Year: <%= f.text_field :release_year %>
    Released: <%= f.check_box :released %>
    Artist Name: <%= f.text_field :artist_name %>
    Genre: <%= f.text_field :genre %>
    <%= f.submit %>
<% end %>
