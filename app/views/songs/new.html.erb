<h1>New Song</h1>

<%= form_for(@song) do |f| %>
  
  <label>Title:</label><br>
  <%= f.text_field :title %><br>

  <label>Released:</label>
  <%= f.check_box :released %>

  <label>Release Year:</label>
  <%= f.number_field :release_year %>

  <label>Artist Name:</label>
  <%= f.text_field :artist_name %>

  <label>Genre:</label>
  <%= f.text_field :genre %>

  <%= f.submit %>

<% end %>