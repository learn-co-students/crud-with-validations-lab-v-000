<h1>New Song form</h1>

<%= form_for(@song) do |f| %>

	<%= f.label :title %>
	<br>
	<%= f.text_field :title%>
	<br>
	<%= f.label :artist_name %>
	<br>
	<%= f.text_field :artist_name %>
	<br>
	<%= f.label :released %>
	<br>
	<%= f.text_field :released %>
 	<br>
	<%= f.label :relase_year %>
	<br>
	<%= f.text_field :release_year %>
	<br>
	<%= f.label :genre %>
	<br>
	<%= f.text_field :genre %>
	<br>
	<%= f.submit "Create Song" %>

<% end %>