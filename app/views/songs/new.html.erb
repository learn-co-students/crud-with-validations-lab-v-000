<h1>Create a New Song</h1>
<%= form_for @song do |f| %>
  Title: <%= f.text_field :title %> <br>
  <div class="radio">
    Released? <%= f.check_box :released, id: "song_released" %> <%= f.label :released, "Yes" %> <br>
  </div>
  Release Year: <%= f.number_field :release_year %> <br>
  Artist: <%= f.text_field :artist_name %> <br>
  Genre: <%= f.text_field :genre %> <br>
  <%= f.submit %>
<% end %>
