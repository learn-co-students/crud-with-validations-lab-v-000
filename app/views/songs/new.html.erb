<h1>Create New Song</h1>
<%= form_for @song do |f| %>
<h3>Song Name:</h3>
<%= f.text_field :title %>
<h3>Artist Name:</h3>
<%= f.text_field :artist_name %>
<h3>Released:</h3>
<%= f.text_field :released %>
<h3>Release Year:</h3>
<%= f.text_field :release_year %>
<h3>Genre:</h3>
<%= f.text_field :genre %> <br>
<%= f.submit %>
<% end %>
