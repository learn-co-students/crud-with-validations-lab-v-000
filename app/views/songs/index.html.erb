<table>
    <thead>
        <tr>
            <th>Song title</th>
            <th>Artist</th>
            <th>Year released</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
        <tr>
            <td><%= link_to song.title, song_path(song) %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.release_year if song.released %></td>
            <td><%= song.genre if song.genre %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<br></br>
<%= link_to "Add New Song", new_song_path %>