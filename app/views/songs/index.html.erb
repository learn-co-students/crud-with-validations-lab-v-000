<p>My Songs</p>
 <table>
   <thead>
     <tr>
       <th>Name</th>
       <th>Released</th>
       <th>Release Date</th>
       <th>Artist Name</th>
       <th>Genre</th>
     </tr>
   </thead>
   <tbody>
     <% @songs.each do |song| %>
     <tr>
       <td><%=song.title%></td>
       <td><%=song.released%></td>
       <td><%=song.release_year%></td>
       <td><%=song.artist_name%></td>
       <td><%=song.genre%></td>
    </tr>
   <% end %>
   <tbody>
 </table>