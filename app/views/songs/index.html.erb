<table>
<tr>
  <th>Title</th>
  <th>Release Year</th>
  <th>Released?</th>
  <th>Genre</th>
  <th>Artist Name</th>
</tr>

<tbody>
<% @songs.each do |song| %>
  <tr>
    <td><%= link_to song.title, song_path(song) %></td>
    <td><%= song.release_year %></td>
    <td><%= song.released %></td>
    <td><%= song.genre %></td>
    <td><%= song.artist_name %></td>
  </tr>
<% end %>
</tbody>
</table>