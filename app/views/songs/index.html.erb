<h1>Songs</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Artist</th>
    <th>Edit</th>
    <th>Delete</th>
  </thead>
  <tbody>
    <%@songs.each do |song|%>
      <tr>
        <td><%=link_to song.title, song_path(song)%></td>
        <td><%=song.artist_name%></td>
        <td><%=link_to "Edit", edit_song_path(song)%></td>
        <td><%=link_to "Destroy", edit_song_path(song)%></td>
      </tr>
    <%end%>
  </tbody>
</table>
