<h1>All Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |s| %>
    <tr>
      <td><%= s.title %></td>
      <td><%= s.release_year %></td>
      <td><%= s.genre %></td>
      <td><%= s.artist_name %></td>
      <td><%= link_to "View", song_path(s) %></td>
      <td><%= link_to "Edit", edit_song_path(s)%></td>
      <td><%= link_to "Delete", s, method: :delete, data: {confirm: "Are you sure?"} %>  </td>
    </tr>
    <% end %>
  </tbody>
</table>
