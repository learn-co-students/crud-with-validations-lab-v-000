<table>
 <thead>
   <tr>
     <th>Song Title</th>
     <th>Song Released</th>
     <th>Year Released</th>
     <th>Artist Name</th>
     <th>Song Genre</th>
     </tr>
     </thead>
     <tbody>
      <% @songs.each do |song| %>
      <tr>
      <td><%= song.title %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
      </tr>
      <% end %>
      </tbody>
      </table>
