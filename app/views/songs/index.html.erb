<h1>Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Artist</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= link_to song.title, song %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
      <td><%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?'} %>
</td>
    </tr>
    <% end %>
  </tbody>
</table><br>
<%= link_to 'New Song', new_song_path %>
