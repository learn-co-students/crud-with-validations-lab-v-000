<h1>All Songs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Genre</th>
    <th>Currently Available?</th>
    <th>Release Year</th>
    <th>Edit</th>
  </tr>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= link_to "Edit", edit_song_path(song) %></td>
      <td><%= link_to 'Delete', @song, method: :delete, data: { confirm: "Really?" } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
