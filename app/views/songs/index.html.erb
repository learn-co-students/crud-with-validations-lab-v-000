<table>
    <thead>
        <tr>
            <th>Song Title</th>
            <th>Artist</th>
            <th>Genre</th>
            <th>Released?</th>
            <th>Release Year</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
                <td><%= song.released.to_s %></td>
                <td><%= song.release_year %></td>
                <td><%= link_to "Edit", edit_song_path(song) %></td>
                <td><%= button_to "Delete", song_path(song), method: :delete %></td>
            </tr>
        <% end %>
    </tbody>
</table>