<h1>Songs</h1>

<div><%= link_to 'Create Song', new_song_path %></div>
<br />



      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist name</th>
      



<tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
        <td>
          <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
