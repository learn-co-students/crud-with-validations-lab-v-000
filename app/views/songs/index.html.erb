<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %><td>
        <td>
          <%= link_to 'View', song %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
