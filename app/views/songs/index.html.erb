<h1> All Songs </h1>
<div><%= link_to 'Create Song', new_song_path %></div>

<table>
 <tbody>
 <% @songs.each do |song| %>
   <tr>
     <td>Title: <%= song.title %> </td>
      <td>Artist: <%= song.artist_name%></td>
       <td>Release Year: <%= song.release_year %></td>
       <td>
          <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
    </tr>
  <%end%>
 </tbody>
 </table>
