<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist</th>
    </tr>
  </thead>

  <tbody>
    <%=@songs.each do |song|%>
      <tr>
        <td><%= song.title%></td>
        <td><%= song.release_year%></td>
        <td><%= song.genre%></td>
        <td><%= song.artist_name%></td>
      </tr>
    <%end%>
  </tbody>
</table>
