<h1>All Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Year</th>
            <th>Genre</th>
            <th>Artist Name</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.release_year %></td>
                <td><%= song.genre %></td>
                <td><%= song.artist_name %></td>
                <td><%= link_to 'View', song %></td>
            </tr>
        <% end %>
    </tbody>
</table>