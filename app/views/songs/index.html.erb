<h3>List of all songs</h3><br>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.release_year %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
        <td><%= link_to "Delete", s, method: :delete, data: { confirm: "Really?" } %></td>
      </tr>
    <% end %>    
  </tbody>
</table>