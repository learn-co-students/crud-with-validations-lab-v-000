<h1>Songs</h1>

<tbody>
    
    <% @songs.each do |song| %>
    <tr>
        <%= song.title %>
        <%= song.released %>
        <%= song.release_year %>
        <%= song.artist_name %>
        <%= song.genre %>
        <%= link_to 'Show', song %>
        <%= link_to 'Edit', edit_song_path(song) %>
        <%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %>
    </tr>
    
<% end %>
    
</tbody>

