<h1>Index</h1>
<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %> | </td>
        <td>
          <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
