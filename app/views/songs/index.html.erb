<table>
  <thead>
    <tr>
      <th>Song Name</th>
      <th>Artist Name</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td><%= song.artist_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
