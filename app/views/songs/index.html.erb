<table>
  <thead>
    <tr>
      <th>
        Title
      </th>
      <th>
        Artist
      </th>
      <th>
        Genre
      </th>
      <th>
        Year Released
      </th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td>
        <%= song.title %>
      </td>
      <td>
        <%= song.artist_name %>
      </td>
      <td>
        <%= song.genre %>
      </td>
      <td>
        <%= song.release_year %>
      </td>
      <td>
        <%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
