<% @songs.each do |song| %>

<table  border>
  <thead>
    <tr>
      <th>Title:</th>
      <th>Year:</th>
      <th>Genre:</th>
      <th>Artist name:</th>
    </tr><br><br>
  </thead>

  <tbody>
   
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
