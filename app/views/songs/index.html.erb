<% @songs.each do |song| %>
  <div class="song">
    <thead>
      <tr>
        <th>Song Title</th>
        <th>Artist Name</th>
        <th>Release Year</th>
        <th>Released</th>
        <th>Genre</th>
      </tr>
    </thead>

    <tbody>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year %></td>
        <td><%= song.released %></td>
        <td><%= song.genre %></td>
      </tr>
    </tbody>
  </div>
<% end %>