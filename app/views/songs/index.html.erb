<head>
  <style>
    thead {color:green;}
    tbody {color:blue;}
    table, th, td {
        border: 1px solid black;
    }
  </style>
</head>

<h1>All Songs</h1>

<ul>
  <table>
    <thead>
    <tr>
      <th>Song Title</th>
      <th>Artist</th>
    </tr>
    <thead>
    <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%=link_to song.title, song_path(song)%></td>
          <td><%=song.artist_name%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</ul>
