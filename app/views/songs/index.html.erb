<h1>All Songs</h1>

<%= @songs.each do |song|%>
  <article>
    <h2><%= song.title%></h2>
    <ul>
      <li>
        <% if song.released %>
          Year Released: <%= song.release_year %>
        <% else %>
          Song Not Released Yet
        <% end %>
      </li>
      <li>Artist Name: <%= song.artist_name %></li>
      <li>Genre: <%= song.genre %></li>
    </ul>
  </article>
<% end %>
