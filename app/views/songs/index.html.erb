<h1>Songs</h1>
<table>
  <tbody>
  <% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.release_year %></td>
    <td><%= song.genre %></td>
    <td><%= song.artist_name %></td>
    <td><%= link_to "View Song", song_path(song) %></td>
    <td><%= link_to "Edit Song", song, method: :edit %></td>
    <td><%= link_to "Delete Song", song, method: :delete, data: { confirm: "Are you sure?" } %></td>
  </tr>
  <% end %>
  </tbody>
</table>
