<h1>All Songs:</h1>

<table>
  <thead>
        <th>Title</th>
        <th>Year</th>
        <th>Genre</th>
        <th>Artist name</th>
        <th></th>
  </thead>

    <tbody>
        <%= @songs.each do |song| %>
            <tr> 
                <%= song.title %>
                <%= song.artist_name %>
                <%= song.release_year %>
                <%= song.genre %>
            </tr> 
        <% end %>
    </tbody>
</table>