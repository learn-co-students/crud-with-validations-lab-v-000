<% if @post.errors.any? %>
  <div >
    <% @post.errors.full_messages.each do |m| %>
    <ul id ="error_explanation">
      <li ><%= m %></li>
    <% end %>
    </ul>
</div>
<% end %
<% @songs.each do |s| %>
  <h3><%= s.title %></h3>
  <h3>s.released</h3>
  <h3>s.release_year</h3>
  <h3>s.artist_name</h3>
  <h3>s.genre</h3>
<% end %>
