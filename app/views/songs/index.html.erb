<h1>Songs Table</h1>

<table>
  <tbody>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
    </tr>
    <% @songs.each do |song| %>
      <td><%= song.title %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
    <% end %>
    <tr></tr>
  </tbody>
</table>
