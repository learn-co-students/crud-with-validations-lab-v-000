<table>
  <thead>
  <tr>
     <th>Title</th>
     <th>Release Year</th>
     <th>Released</th>
     <th>Genre</th>
     <th>Artist Name</th>
  </tr>
  </thead>
  <tbody>
    <%@songs.each do |song|%>
      <tr>
        <td><%=song.title%></td>
        <td><%=song.release_year%></td>
        <td><%=song.released%></td>
        <td><%=song.genre%></td>
        <td><%=song.artist_name%></td>
        <%= link_to 'Show', song %>
        <%= link_to 'Edit', edit_song_path(song) %>
        <%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %>
      </tr>
    <%end%>
  </tbody>
</table>
