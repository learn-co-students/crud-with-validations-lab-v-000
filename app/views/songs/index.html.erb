<h1>Songs: </h1>

<table id="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %> </td>
      <td><%= song.release_year %> </td>
      <td><%= song.artist_name %> </td>
      <td><%= song.genre %> </td>
      <td>
        <%= link_to 'View', song_path(song) %> /
        <%= link_to 'Edit', edit_song_path(song) %> /
        <%= link_to 'Delete', song, method: :delete, data: { confirm: 'confirm delete?' } %>
      </td>
    </tr>
    <% end %>
  </tbody>

</table>
