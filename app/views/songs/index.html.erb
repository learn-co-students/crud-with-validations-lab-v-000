<h1>All Songs</h1>
<div>
  <table>
    <thead>
      <tr>
        <th>
          Title
        </th>
        <th>
          Year
        </th>
      </tr>
    </thead>

    <tbody>
      <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
