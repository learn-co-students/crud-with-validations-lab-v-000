<h1>Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Genre</th>
      <th>Released</th>
      <th>Release Year</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <th><%= song.title %></th>
        <th><%= song.artist_name %></th>
        <th><%= song.genre %></th>
        <th><%= song.released %></th>
        <th><%= song.release_year %></th>
      </tr>
    <% end %>
  </tbody>
</table>
