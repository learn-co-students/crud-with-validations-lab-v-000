<h2>List of all Songs:</h2>

<table>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Release Year</th>
    <th>Genre</th>
    <th></th>
  </tr>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= link_to "Delete", song, method: :delete, data: { confirm: "Are you sure you want to delete?" } %>
      </tr></td>
    <% end %>


  </tbody>
</table>
