<h1>List of Songs</h1>
<table>


<thead>
  <tr>
    <th>Title</th>
    <th>Year</th>
    <th>Artist</th>
    <th>Genre</th>
    <th></th>
  </tr>
</thead>

<tbody>
  <% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.release_year %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
    <td>
      <%= link_to "View", song %> | <%= link_to "Edit", edit_song_path(song) %> | <%= link_to "Delete", song, method: :delete, data: {confirm: "Are you sure?"} %> | <%= link_to "Create New Song", new_song_path %>
    </td>
  </tr>
  <% end %>
</tbody>
</table>
