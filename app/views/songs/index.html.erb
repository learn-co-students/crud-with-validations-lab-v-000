<h1>Song Index</h1>
<tbody>
<%Song.all.each do |song| %>
  <tr>
    <td>Title: <%=link_to song.title, song_path(song)%></td>
    <td>Artist: <%=song.artist_name %></td>
    <td>Released? <%=song.released%></td>
    <td>Release Year: <%=song.release_year%></td>
    <td>Genre: <%=song.genre %></td>
  </tr>
<%end%>


<p><%=link_to "Create a New Song", new_song_path%></p>
