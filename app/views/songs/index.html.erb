<h1> List of Songs </h1>
<table>
  <tbody>
<% @songs.each do |song| %>
<tr>
  <%= link_to song.title, song_path(song) %>
  <%= link_to 'Edit', edit_song_path(song) %>
  <%= link_to 'Delete', song_path(song), method: :destroy, data: { confirm: "Confirm delete?"} %></tr>
  <% end %>
</tbody>
</table>
