Index of Songs


<table>
  <thead>
  <tr>
     <th>Title</th>
     <th>Artist Name</th>
     <th>Released</th>
     <th>Release Year</th>
  </tr>
  </thead>
  
  <tbody>
       <% @songs.each do |song|%>
  <tr>
     <td><%= link_to song.title, song_path(song)%></td>
     <td><%= song.artist_name %></td>
     <td><%= song.released %></td>
     <td><%= song.release_year %></td>
  </tr>
  <% end %>
  
  </tbody>
</table>