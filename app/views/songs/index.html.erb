<h1>All songs</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Artist name</th>
    <th>Genre</th>
  </tr>
</thead>
<tbody>
   <% @songs.each do |song| %>
     <tr>
       <td><%= song.title %></td>
       <td><%= song.released %></td>
       <td><%= song.release_year %></td>
       <td><%= song.artist_name %></td>
       <td><%= song.genre %></td>
     </tr>
   <% end %>
 </tbody>
</table>
