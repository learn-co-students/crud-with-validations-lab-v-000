<h1>All Songs</h1>
<%= link_to 'New Song', new_song_path %>
<table border = "1">
		<tr>
        <th>Title</th>
        <th>Artist Name</th>
        <th>Edit</th>
        <th>Delete</th>
		</tr>
         
    <% @songs.each do |song| %>
    <tr>
      <td><%= link_to song.title, song_path(song) %></td>
      <td><%= song.artist_name %></td>
      <td><%= link_to 'Edit', edit_song_path(song) %></td>
      <td><%= link_to 'Delete', delete_song_path(song) %></td>
    </tr>
 		<% end %>
                 
 </table>