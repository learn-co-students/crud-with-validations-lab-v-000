
<table style="width:100%">
  <tr>
    <th>Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Genre</th>
    <th>Artist Name</th>
  </tr>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
      </tr>
    <% end %>
  </tbody>

</table>
