<h1>All Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.title%></td>
      <td><%= song.release_year %></td>
      <td><%= song.genre %></td>
      <td><%= song.artist_name %></td>
      <td><%= link_to "Delete", song, method: :delete, data: {confirm: "Are you sure?"} %></td>
      <td><%= link_to "Edit", edit_song_path(song) %></td>
      <td><%= link_to "Show", song_path(song) %></td>
    </tr>


    <% end %>
  </tbody>
</table>
<p><%= link_to "Create Song", new_song_path%></p>
