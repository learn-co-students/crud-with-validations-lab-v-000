<table>
  <tr>
    <th>Title  |</th>
    <th>released  |</th>
    <th>release_year  |</th>
    <th>artist name  |</th>
    <th>genre  |</th>
  </tr>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>
