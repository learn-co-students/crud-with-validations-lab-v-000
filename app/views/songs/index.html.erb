<table class="table table-striped">
  <thead>
    <th>Song</th>
    <th>Artist</th>
    <th>Genre</th>
    <th>Released</th>
    <th>Release Year</th>
  </thead>
  <tbody>
    <tr>
      <% @songs.each do |song| %>
        <tr>
          <td><%= link_to song.title, song_path(song) %></td>
          <td><%= song.artist_name %></td>
          <td><%= song.genre %></td>
          <td><%= song.released %></td>
          <td><%= song.release_year %></td>
          </td>
        <% end %>
      </tr>
    </tr>
  </tbody>
</table>
