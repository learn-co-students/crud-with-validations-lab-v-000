<h2>Songs</h2>


<table>
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Release Year</th>
      <th>Released</th>
      <th>Genre</th>
      <th>Artist Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>

    <% @songs.each do |song| %>
    <tr>
      <td><%=song.title%></td>
      <td><%=song.release_year%></td>
      <td><%=song.released%></td>
      <td><%=song.genre%></td>
      <td><%=song.artist_name%></td>
      <td>
        <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?' } %>
      </td>
      <% end %>
    </tr>
  </tbody>
</table>
