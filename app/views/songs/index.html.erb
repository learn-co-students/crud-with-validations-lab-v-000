<h1>All Songs</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Artist Name</th>
    <th>Release Year</th>
    <th>Genre</th>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>
