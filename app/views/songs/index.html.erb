<h1>Songs Index</h1>

<table border="1">
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Artist</th>
      <th>Release Year</th>
      <th>Genre</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= link_to song.title, song_path(song) %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.release_year %></td>
      <td><%= song.genre %></td>
    </tr>
    <% end %>
  </tbody>
</table>
