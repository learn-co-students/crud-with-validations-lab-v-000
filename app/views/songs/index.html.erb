<h1>Your Songs</h1>
<table style="width:50%">
    <thead>
        <tr align="left">
            <th>Song</th>
            <th>Artist</th>
            <th>Genre</th>
            <th> Year</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
                <td><%= song.release_year %></td>
                <td>
                    <%= link_to 'View this Song', song_path(song) %> |
                    <%= link_to 'Edit', edit_song_path(song) %> |
                    <%= link_to 'Delete', song, method: :delete, data: { confirm: "Are you sure you want to delete '#{song.title}'?" } %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>