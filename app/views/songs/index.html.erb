<h1>All Songs</h1>
<table>
    <tr>
        <th>Title</th>
        <th>Released?</th>
        <th>Year Released</th>
        <th>Artist</th>
        <th>Genre</th>
    </tr>
    <% @songs.each do |song| %>
    <tbody>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.released%></td>
            <td><%= song.release_year%></td>
            <td><%= song.artist_name%></td>
            <td><%= song.genre%></td>
            <td><%= link_to "Delete", song, method: :delete, data: { confirm: "Are you sure?" } %></td>
        </tr>
    <%end%>
    <tbody>
</table>