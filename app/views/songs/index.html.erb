<h1>Songs</h1>

<table>

<thead>
  <tr>
    <th>
      Title
    </th>
    <th>
      Release year
    </th>
    <th>
      Released?
    </th>
    <th>
      genre
    </th>
    <th>
      Artist Name
    </th>
  </tr>
</thead>

<tbody>
<% @songs.each do |song| %>
<tr>
  <%= song.title %><br>
  <%= song.release_year %><br>
  <%= song.genre %><br>
  <%= song.artist_name %><br>
  <%= song.released %><br>
</tr>

  <%= link_to "Delete", song, method: :delete, data: { confirm: 'Really?'} %>

<% end %>

</tbody>
</table>
