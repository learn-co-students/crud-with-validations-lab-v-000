<html>
<head>
  <title></title>
</head>
<body>
  <h1>All songs</h1>
  <table>

  <tr>
    <th>Song</th>
    <th>Year</th>
  </tr>
  <% @songs.each do |song| %>
    <tbody>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.release_year %></td>
    </tr>
  <% end %>
</tbody>

</table>
</body>
</html>