<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %> </td>
        <td><%= song.released %> </td>
        <td><%= song.release_year %> </td>
        <td><%= song.genre %> </td>
        <td><%= song.artist_name %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
