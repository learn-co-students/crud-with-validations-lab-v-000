<h1>All Songs</h1>

<table>
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Artist Name</th>
      <th>Genre</th>
      <th>Release Year</th>
      <th>Released?</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
        <td><%= s.release_year %></td>
        <td><%= s.released? %></td>
      </tr>
    <% end %>
  </tbody>
</table>
