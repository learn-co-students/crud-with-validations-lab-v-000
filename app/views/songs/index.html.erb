<%= link_to "New Song", new_song_path %><br>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Genre</th>
      <th>Year</th>
      <th></th>
    </tr>
  </thead>

  <!--- Talk about some specific formatting breaking tests... !-->

  <tbody>
  <% @songs.each do |song| %>
  <tr>
<td><%= song.title %></td>
<td><%= song.artist_name %></td>
<td><%= song.genre %></td>
<td><%= song.release_year %></td>
<td>
    <%= link_to 'View', song %> |
    <%= link_to 'Edit', edit_song_path(song) %> |
    <%= link_to 'Delete', song, method: :delete, data: { confirm: 'R U Sure?' } %>
  </td>
  </tr>
  <% end %>
</tbody>
</table>