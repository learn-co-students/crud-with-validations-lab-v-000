<h1>Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist Name</th>
      <th>Year</th>
      <th>Genre</th>
    </tr>

<tbody>
  <% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.release_year %></td>
    <td><%= song.genre %></td>
  </tr>
  <td>
  <%= link_to 'View', song_path(song) %>
  <%= link_to 'Edit', edit_song_path(song) %>
</td>
    <% end %>
  </tbody>
</table>
