<h2> Songs List</h2>
<div><%= link_to 'Add Song', new_song_path %></div>
<% @songs.each do |song| %>
<tbody>
   <tr>
     <label>Song Title: </label>
     <%= link_to song.title, song_path(song)%><br>
     <%= link_to "Edit", edit_song_path(song)%>
     <%= link_to "Delete", song, method: :delete, data:{confirm: "Are you sure?"} %>
   </tr>
 </tbody>
<% end %>
