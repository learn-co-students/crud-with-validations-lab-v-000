<h2>My Songs</h2>

<table>
  <tr>
    <th>Title</th>
    <th>Artist Name</th>
    <th>Release Year</th>
    <th>Genre</th>
    <th>Options</th>
  </tr>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td>
          <%= link_to 'View', song_path(song) %>
          <%= link_to 'Edit', edit_song_path(song) %>
          <%= link_to 'Delete', song, method: :delete, data: { confirm: "Are you sure?" } %>
        </td>
      </tr>
    <% end %>
  </tbody>

</table>