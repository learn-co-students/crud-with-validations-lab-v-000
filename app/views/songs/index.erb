<h1>Song List</h1>
<table style="width: 25%">
  <tr>
    <th>Title</th>
    <th>Released</th>
    <th>Released Year</th>
    <th>Artist</th>
    <th>Genre</th>
  </tr>
<% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.released %></td>
    <td><%= song.release_year %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
  </tr>
<% end %>
</table>
<br>
