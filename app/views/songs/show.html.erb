<h1>Song</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Artist</th>
    <th>Released?</th>
    <th>Year Released</th>
    <th>Genre</th>
  </thead>
  <tbody>
    <tr>
      <td><%= @song.title %></td>
      <td><%= @song.artist_name %></td>
      <td><%= @song.released %></td>
      <td><%= @song.release_year %></td>
      <td><%= @song.genre %></td>
    </tr>
  </tbody>
</table>
