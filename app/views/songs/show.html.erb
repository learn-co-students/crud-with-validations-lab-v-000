<h2><%= @song.title %></h2>
<div>
  <p>Releaseed Year: <%= @song.release_year %></p>
  <p>Artist Name: <%= @song.artist_name %></p>
  <p>Genre: <%= @song.genre %></p>
</div>
<%= link_to "Edit", edit_song_path(@song) %>
<%= link_to 'Delete', @song, :method => :destroy, :data => {:confirm => "You Sure?"}  %>
