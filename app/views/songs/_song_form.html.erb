<%= form_for @song do |f| %>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %><br>
  <%= f.label :released, "Released" %>
  <%= f.check_box :released %><br>
  <%= f.label :release_year, "Release Year" %>
  <%= f.text_field :release_year %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name%><br>
  <%= f.label :genre %>
  <%= f.text_field :genre %><br>
  <%= f.submit %>
<% end %>
