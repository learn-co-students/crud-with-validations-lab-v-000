<%= form_for(@song) do |f| %>
  <%= f.label :title %><br></br>
  <%= f.text_field :title %><br></br>

  <%= f.label :released %><br></br>
  <%= f.check_box :released %><br></br>

  <%= f.label :release_year %><br></br>
  <%= f.text_field :release_year %><br></br>

  <%= f.label :artist_name %><br></br>
  <%= f.text_field :artist_name %><br></br>

  <%= f.label :genre %><br></br>
  <%= f.text_field :genre %><br></br>

  <%= f.submit %>
<% end %>
