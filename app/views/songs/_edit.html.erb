<%= form_for @song do |f|%>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
    <%= f.label :release_year %>
    <%= f.text_field :release_year %>
    <%= f.check_box :released %>
    <%= f.submit %>
<% end %>
